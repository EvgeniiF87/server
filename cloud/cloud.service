[Unit]
Description=cloud

[Service]
ExecStart=/root/server/cloud/dist/main.js
Restart=always
User=nobody
# Note Debian/Ubuntu uses 'nogroup', RHEL/Fedora uses 'nobody'
Group=nogroup
Environment=PATH=/usr/bin:/usr/local/bin
WorkingDirectory=/root/server/cloud/dist

[Install]
WantedBy=multi-user.target